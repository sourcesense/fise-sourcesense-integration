<atlassian-plugin name='IKS-FISE Confluence integration' key='eu.iksproject.fise.sourcesense.confluence'>
   <plugin-info>
      <description>IKS-FISE Confluence integration</description>
      <vendor name="Sourcesense" url="http://www.sourcesense.com" />
      <version>1.0</version>
   </plugin-info>

   <listener name='IKS-FISE Page creation Listener' class='eu.iksproject.fise.sourcesense.confluence.BaseIKSEnhancementPageListener'
      key='fise-page-listener'>
      <description>Everytime a page is created inside Confluence, also some semantic metadata extraction is extracted and saved via IKS FISE.</description>
   </listener>

  <component key="enrichmentEnginesExecutor" class="eu.iksproject.fise.sourcesense.confluence.OpenNLPEngineEnrichmentEnginesExecutor">
      <description>Autotagging service implementation</description>
      <interface>eu.iksproject.fise.sourcesense.confluence.EnrichmentEnginesExecutor</interface>
  </component>

</atlassian-plugin>

