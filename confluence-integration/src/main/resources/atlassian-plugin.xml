<atlassian-plugin name='IKS-FISE Confluence integration' key='eu.iksproject.fise.sourcesense.enhance'>
   <plugin-info>
      <description>IKS-FISE Confluence integration</description>
      <vendor name="Sourcesense" url="http://www.sourcesense.com" />
      <version>1.0</version>
   </plugin-info>

   <listener name='IKS-FISE Page creation Listener' class='eu.iksproject.fise.sourcesense.enhance.BaseIKSEnhancementPageListener'
      key='fise-page-listener'>
      <description>Everytime a page is created inside Confluence, also some semantic metadata extraction is extracted and saved via IKS FISE.</description>
   </listener>

  <component key="enrichmentEnginesExecutor" class="${executor.implementation}">
      <description>Autotagging service implementation</description>
      <interface>eu.iksproject.fise.sourcesense.enhance.EnrichmentEnginesExecutor</interface>
  </component>

  <component key="tagCleaner" class="eu.iksproject.fise.sourcesense.enhance.SimpleTagCleaner">
    <description>Tag cleaning policy</description>
    <interface>eu.iksproject.fise.sourcesense.enhance.TagCleaner</interface>
  </component>

</atlassian-plugin>

